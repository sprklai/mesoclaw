{
  "name": "mesoclaw",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "branding:sync": "node scripts/sync-branding.mjs",
    "build": "bun run generate-routes && tsc && vite build",
    "check": "bun run generate-routes && tsc --noEmit",
    "dev": "concurrently -k \"bun run watch-routes\" \"vite\"",
    "predev": "bun run branding:sync",
    "format": "bunx ultracite fix",
    "generate-routes": "tsr generate",
    "lint": "bunx ultracite check",
    "precheck": "bun run branding:sync",
    "preview": "vite preview",
    "tauri": "tauri",
    "pretauri:dev": "bun run branding:sync",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest run --ui",
    "test:watch": "vitest watch",
    "validate": "turbo run build check lint test",
    "watch-routes": "tsr watch",
    "tauri:dev": "WEBKIT_DISABLE_DMABUF_RENDERER=1 tauri dev",
    "tauri:dev:fast": "WEBKIT_DISABLE_DMABUF_RENDERER=1 TAURI_DEV_WATCH=true tauri dev",
    "tauri:build": "tauri build",
    "pretauri:build": "bun run branding:sync",
    "tauri:build:fast": "cargo tauri build --profile release-fast",
    "prebuild": "bun run branding:sync",
    "cargo:check": "cd src-tauri && cargo check",
    "cargo:build": "cd src-tauri && cargo build",
    "cargo:build:release": "cd src-tauri && cargo build --release",
    "cargo:clean": "cd src-tauri && cargo clean",
    "build:all": "./scripts/cross-platform-build.sh",
    "build:default": "./scripts/cross-platform-build.sh --default",
    "build:linux": "./scripts/cross-platform-build.sh --linux",
    "build:windows": "./scripts/cross-platform-build.sh --windows",
    "build:macos": "./scripts/cross-platform-build.sh --macos",
    "build:docker": "./scripts/docker-build.sh",
    "build:docker-windows": "./scripts/docker-build.sh --windows",
    "build:docker-linux": "./scripts/docker-build.sh --linux"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.51",
    "@base-ui/react": "^1.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@lobehub/icons": "^4.0.3",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-router": "^1.151.2",
    "@tanstack/react-table": "^8.21.3",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-autostart": "^2.5.1",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-log": "^2.8.0",
    "@tauri-apps/plugin-notification": "^2.3.3",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@tauri-apps/plugin-store": "^2.4.2",
    "@tauri-apps/plugin-stronghold": "^2.3.1",
    "@tauri-apps/plugin-window-state": "^2.4.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "i18next": "^25.8.10",
    "i18next-browser-languagedetector": "^8.2.1",
    "i18next-icu": "^2.4.3",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.1.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "react-hotkeys-hook": "^5.2.4",
    "react-i18next": "^16.5.4",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-social-icons": "^6.25.0",
    "react-syntax-highlighter": "^16.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tanstack/router-cli": "^1.151.2",
    "@tanstack/router-plugin": "^1.151.2",
    "@tauri-apps/cli": "^2.9.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "color-contrast-checker": "^2.1.0",
    "concurrently": "^9.2.1",
    "happy-dom": "^20.3.9",
    "oxlint": "^1.39.0",
    "turbo": "^2.7.5",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "ultracite": "^7.0.11",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  },
  "packageManager": "bun@1.3.6"
}
